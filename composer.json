{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-curl": "*",
        "ext-json": "*",
        "beyondcode/laravel-mailbox": "^1.4",
        "beyondcode/laravel-websockets": "^1.6",
        "consoletvs/charts": "6.*",
        "encore/laravel-admin": "^1.7",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.5",
        "hootlex/laravel-friendships": "^1.0.1",
        "jeroennoten/laravel-adminlte": "^3.2",
        "joshbrw/laravel-module-installer": "^0.1.4",
        "laravel-admin-ext/summernote": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/passport": "^9.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "lexxyungcarter/chatmessenger": "^1.3",
        "mews/captcha": "^3.1",
        "nao-pon/flysystem-google-drive": "~1.1",
        "nwidart/laravel-modules": "^7.1",
        "overtrue/laravel-like": "^2.0",
        "phpunit/phpunit": "^8.5",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^4.1",
        "realrashid/sweet-alert": "^3.0",
        "sentry/sentry-laravel": "^1.8",
        "spatie/laravel-backup": "^6.10",
        "spatie/laravel-permission": "^3.11",
        "spatie/regex": "^1.4.0",
        "stevebauman/location": "^5.1",
        "stripe/stripe-php": "^7.37",
        "thomaswelton/laravel-gravatar": "~1.0"
    },
    "require-dev": {
        "appzcoder/crud-generator": "^3.2",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
